{
    "version": 2,
    "env": {
      "CUSTOM_ENV_VARIABLE": "value"
    },
    "builds": [{
      "src": "package.json",
      "use": "@vercel/node"
    }],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  